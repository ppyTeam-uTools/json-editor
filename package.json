{
  "name": "json-editor",
  "version": "0.0.1",
  "description": "uTools JSON 编辑器",
  "keywords": [
    "uTools",
    "JSON",
    "editor"
  ],
  "author": "ttionya <git@ttionya.com>",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "pnpm check:type && vite build",
    "check:type": "vue-tsc --noEmit",
    "lint": "eslint src --ext .vue,.js,.jsx,.ts,.tsx --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@ttionya/eslint-config": "^2.3.0",
    "@ttionya/prettier-config": "^1.1.0",
    "@types/node": "^18.11.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "sass": "^1.56.0",
    "typescript": "^4.9.0",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.0"
  },
  "dependencies": {
    "monaco-editor": "^0.34.0",
    "vue": "^3.2.45"
  }
}
